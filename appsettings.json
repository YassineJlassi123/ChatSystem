{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "ENV_DEFAULT_CONNECTION" // Placeholder for environment variable
  },
  "Jwt": {
    "Key": "ENV_JWT_KEY", // Placeholder for environment variable
    "Issuer": "ENV_JWT_ISSUER", // Placeholder for environment variable
    "Audience": "ENV_JWT_AUDIENCE" // Placeholder for environment variable
  }
}